Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{"Set", " ", "initial", " ", "point"}], " ", "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"x", "=", "0"}], ";", 
    RowBox[{"y", "=", "0"}], ";", " ", 
    RowBox[{"z", "=", "0"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"pts", "=", 
     RowBox[{"{", 
      RowBox[{"{", 
       RowBox[{"x", ",", "y", ",", "z"}], "}"}], "}"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"counter", "=", "0"}], ";"}], "\[IndentingNewLine]", 
   RowBox[{"While", "[", 
    RowBox[{
     RowBox[{"counter", "<", "Njumps"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{
       "**", "**", "**", "**", "**", "**", "**", "**", "**", "**", "**", "**",
         "**", "**", "\[IndentingNewLine]", "Generation"}], " ", "new", " ", 
       "point", " ", 
       RowBox[{
        RowBox[{"x", "'"}], "\[IndentingNewLine]", "**", "**", "**"}], "*", 
       RowBox[{"**", "**", "**", "**", "**", "**", "**", "**"}]}], "*****)"}],
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Generation", " ", "random", " ", "uniform", " ", "angles", " ", 
       "\[Theta]", " ", "and", " ", "\[Phi]"}], " ", "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"phi", "=", 
       RowBox[{"RandomReal", "[", 
        RowBox[{"2", "\[Pi]"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"costh", "=", 
       RowBox[{"RandomReal", "[", 
        RowBox[{"{", 
         RowBox[{
          RowBox[{"-", "1"}], ",", "1"}], "}"}], "]"}]}], ";", 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
        RowBox[{"Generation", " ", "r"}], "=", 
        RowBox[{"|", 
         RowBox[{
          RowBox[{"x", "'"}], "-", "x"}], 
         SuperscriptBox["|", " "], 
         RowBox[{
         "with", " ", "truncated", " ", "Gamma", " ", "distribution"}]}]}], 
       " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"\[Alpha]", "=", 
       RowBox[{"m", "-", 
        RowBox[{"\[Lambda]", " ", "costh"}]}]}], ";", "   ", 
      RowBox[{"(*", " ", 
       RowBox[{"Always", " ", "positive", " ", "number"}], " ", "*)"}], 
      "\[IndentingNewLine]", 
      RowBox[{"rho", "=", 
       RowBox[{"Sqrt", "[", 
        RowBox[{
         SuperscriptBox["x", "2"], "+", 
         SuperscriptBox["y", "2"]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"rstar", "=", 
       RowBox[{
        RowBox[{"Sqrt", "[", 
         RowBox[{
          SuperscriptBox["R", "2"], "+", 
          SuperscriptBox["rho", "2"], "-", 
          RowBox[{"2", " ", "R", " ", "rho", " ", 
           RowBox[{"Cos", "[", "phi", "]"}]}]}], "]"}], "/", 
        RowBox[{"Sqrt", "[", 
         RowBox[{"1", "-", 
          SuperscriptBox["costh", "2"]}], "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"\[Beta]", "=", 
       RowBox[{"1", "-", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"1", "+", 
           RowBox[{"\[Alpha]", " ", "rstar"}]}], ")"}], 
         RowBox[{"Exp", "[", 
          RowBox[{
           RowBox[{"-", "\[Alpha]"}], " ", "rstar"}], "]"}]}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"w", "=", 
       RowBox[{"RandomReal", "[", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"r", "=", 
       RowBox[{
        FractionBox[
         RowBox[{"t", "-", "1"}], "\[Alpha]"], "/.", 
        RowBox[{"Last", "[", 
         RowBox[{"Quiet", "[", 
          RowBox[{"FindRoot", "[", 
           RowBox[{
            RowBox[{
             RowBox[{"t", " ", 
              SuperscriptBox["E", 
               RowBox[{"-", "t"}]]}], "-", 
             RowBox[{
              RowBox[{"(", 
               RowBox[{"1", "-", 
                RowBox[{"\[Beta]", " ", "w"}]}], ")"}], "/", "E"}]}], ",", 
            RowBox[{"{", 
             RowBox[{"t", ",", "2"}], "}"}]}], "]"}], "]"}], "]"}]}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"xnew", "=", 
       RowBox[{"x", "+", 
        RowBox[{"r", " ", 
         RowBox[{"Sqrt", "[", 
          RowBox[{"1", "-", 
           SuperscriptBox["costh", "2"]}], "]"}], 
         RowBox[{"Cos", "[", "phi", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"ynew", "=", 
       RowBox[{"y", "+", 
        RowBox[{"r", " ", 
         RowBox[{"Sqrt", "[", 
          RowBox[{"1", "-", 
           SuperscriptBox["costh", "2"]}], "]"}], 
         RowBox[{"Sin", "[", "phi", "]"}]}]}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"znew", "=", 
       RowBox[{"z", "+", 
        RowBox[{"r", " ", "costh"}]}]}], ";", "\[IndentingNewLine]", 
      "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{"Print", "[", 
         RowBox[{"\"\<\[Phi] = \>\"", ",", 
          RowBox[{"phi", "/", "\[Pi]"}], ",", "\"\< * \[Pi]\>\""}], "]"}], 
        ";", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\<Cos[\[Theta]] = \>\"", ",", "costh"}], "]"}], ";", " ", 
        
        RowBox[{"Print", "[", 
         RowBox[{"\"\<\[Alpha] = \>\"", ",", " ", "\[Alpha]"}], "]"}], ";", 
        "  ", 
        RowBox[{"Print", "[", 
         RowBox[{
         "\"\<\!\(\*SubscriptBox[\(r\), \(*\)]\) = \>\"", ",", " ", "rstar"}],
          "]"}], ";", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\<\[Beta] = \>\"", ",", " ", "\[Beta]"}], "]"}], ";", 
        "  ", 
        RowBox[{"Print", "[", 
         RowBox[{"\"\<r = \>\"", ",", " ", "r"}], "]"}], ";"}], "*)"}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{
        RowBox[{
        "**", "**", "**", "**", "**", "**", "**", "**", "**", "**", "**", "**",
          "**", "**", "\[IndentingNewLine]", "Rejection"}], " ", 
        RowBox[{"sampling", "\[IndentingNewLine]", "**", "**", "**"}], "*", 
        RowBox[{"**", "**", "**", "**", "**", "**", "**", "**"}]}], 
       "******)"}], "\[IndentingNewLine]", 
      RowBox[{"rhonew", "=", 
       RowBox[{"Sqrt", "[", 
        RowBox[{
         SuperscriptBox["xnew", "2"], "+", 
         SuperscriptBox["ynew", "2"]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"RandomReal", "[", "]"}], "<", 
         RowBox[{"BesselJ", "[", 
          RowBox[{"0", ",", 
           RowBox[{"\[Lambda]", " ", "rhonew"}]}], "]"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Module", "[", 
         RowBox[{
          RowBox[{"{", "}"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"counter", "++"}], ";", "\[IndentingNewLine]", 
           RowBox[{"x", "=", "xnew"}], ";", 
           RowBox[{"y", "=", "ynew"}], ";", 
           RowBox[{"z", "=", "znew"}], ";", "\[IndentingNewLine]", 
           RowBox[{"pts", "=", 
            RowBox[{"Join", "[", 
             RowBox[{"pts", ",", 
              RowBox[{"{", 
               RowBox[{"{", 
                RowBox[{"x", ",", "y", ",", "z"}], "}"}], "}"}]}], "]"}]}], 
           ";"}]}], "\[IndentingNewLine]", "]"}]}], "\[IndentingNewLine]", 
       "]"}], ";"}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}], 
